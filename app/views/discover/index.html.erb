<%= render partial: "shared/nav" %>

<body>
<h1>Discover Movies</h1>

<%= button_to "Discover Top Rated Movies", user_movies_path(@user) %>

<%= form_with url: user_movies_path(@user), method: :get do |f| %>
   <%= f.label :search, "Search by Title" %>
   <%= f.text_field :search %>
   <%= f.submit "Search" %>
<% end %>
</body>