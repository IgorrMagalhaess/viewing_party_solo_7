<h1>Viewing Party</h1>
<% if current_user %>
   <%= button_to "Log Out", class: "btn btn-lg btn-outline-secondary" %>
<% else %>
   <%= button_to "Login", login_path, method: :get, class: "btn btn-lg btn-outline-secondary" %>
   <%= button_to "Create New User", register_user_path, method: :get, class: "btn btn-lg btn-outline-secondary" %>
<% end %>

<h2>Existing Users</h2>
<ul id="existing_users">
   <% @users.each do |user| %>
      <li><%= link_to user.email, "users/#{user.id}" %></li>
   <% end %>
</ul>
